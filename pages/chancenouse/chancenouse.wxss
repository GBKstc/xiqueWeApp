}

.produce{
  color: #999999;
  font-size: 32rpx;
  display: inline-block;
  margin-top: 104rpx;
}
.produce_img{
  width:133rpx;
  height: 7rpx;
  display: block;
  margin-top: 18rpx;
}
.produce_top{
  margin-top: 61rpx;
}
.five_logo{
  color: #333333;
  font-size: 32rpx;
  margin-top: 37rpx;
}
.appoint{
  font-size: 28rpx;
  color: #999999;
}
.five_right{
  float: right;
  color: #333333;
}
.gobtn{
  background: #FD9595;
  color: #fff;
  border-radius: 0px;
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
}
.buyBox{
  display: flex;
  align-items: center;
  justify-content: flex-start;
  margin-top: 37rpx;
}
.buyTitle{
  width:165rpx;
