  line-height: 32rpx;
  color: #333333;
}
.consumeDetailList .consumeDetail{
  padding-bottom: 1rpx;
}
.consumeDetailTitle{
  display: flex;
  justify-content: start;
  align-items: flex-end;
  font-size:32rpx;
  line-height: 32rpx;
  margin-top: 30rpx;
  margin-bottom: 80rpx;
  padding-bottom: 15rpx;
  position: relative;
}
.consumeDetailTitle::after{
  content: "";
  display: block;
  width:40rpx;
  height:4rpx;
  background:rgba(253,149,149,1);
  border-radius:2rpx;
  position: absolute;
  bottom: 0;
  left: 0;
}
