}
.take_left{
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1; 
}
.count{
  margin-top: 30rpx;
  display:flex;
  justify-content:flex-start;
  align-items:center;
}

