/*
  浮动框
*/
.shadow{
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  background:rgba(0,0,0,0.5);
}
.modal{
  transition: all 0.8s;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: white;
  border-radius:10rpx 10rpx 0px 0px;
}

.consumeDetailList{
  margin: 0 auto;
  padding: 0 60rpx;
  
}
.consumeDetailList .detailItem{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 30rpx;
}
.consumeDetailList .detailItem .title{
  display: flex;
  justify-content: start;
  align-items: flex-end;
  font-size: 32rpx;
  line-height: 32rpx;
  color: #666666;
}
.consumeDetailList .titleRemark{
  font-size:28rpx;
  color:rgba(153,153,153,1);
  margin-left: 20rpx;
  line-height: 28rpx;
}
.consumeDetailList .detailItem .number{
