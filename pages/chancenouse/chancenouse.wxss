  color:#333333;
  line-height:32rpx;
}

.refund{
  width:100rpx;
  background:rgba(255,255,255,1);
  border: 1px solid rgba(153,153,153,1);
  border-radius:6rpx;
  color: rgba(153,153,153,1);
  font-size: 28rpx;
  line-height: 28rpx;
  padding: 11rpx 0;
  /* margin-top: 25rpx; */
  text-align: center;
  margin-bottom: 22px;
}