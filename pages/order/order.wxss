@import "../../utils/commonConfirm.wxss";
.tabTop{
  position:fixed;
  top:0;
  left:0;
  height:100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #f5f5f5;
  font-size:30rpx;
  color:#333;
  background-color: #fff;
  z-index: 999;
}
.changeBlock{
  display: inline-block;
  width:33.33vw; 
  text-align: center;
}
.swiperCellActive{
  color:#fd9595;
  border-bottom:2px solid #fd9595;
}
.list{
  width:630rpx;
  padding: 100rpx 60rpx 0 60rpx;
  position: fixed;
  height: 100%;
  overflow: auto;
  top: 0;
  left: 0;
}
.noOrderText{
  margin-top: 500rpx;
  text-align: center;
  font-size:32rpx;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.listItem{
  padding:60rpx 0;
  box-sizing: border-box;
  border-bottom:1px solid #f5f5f5;
}
.up .date{
  color:#333;
  font-size:40rpx;
  margin-right:20rpx;
  vertical-align: middle;
}
.down .week{
  color:#b1b1b1;
  font-size:28rpx;
  margin-left:20rpx;
  vertical-align: middle;
}
.up .time{
  color:#b1b1b1;
  font-size:28rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}
.down .time{
  color:#b1b1b1;
  font-size:28rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}
.up .noon{
  color:#b1b1b1;
  font-size:28rpx;
  vertical-align: middle;
}
.down{
  color:#333;
  font-size:40rpx;
  margin-top:30rpx;
}
.evaluate{
  width:120rpx;
  height:50rpx;
  line-height: 50rpx;
  display: inline-block;
  color:#fd9595;
  font-size:24rpx;
  border:1px solid #fd9595;
  border-radius: 6rpx;
  text-align: center;
  float:right;
}