/*
  浮动框
*/
.shadow{
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  background:rgba(0,0,0,0.5);
}
.modal{
  transition: all 0.8s;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 80%;
  overflow: auto;
  background-color: white;
  border-radius:10rpx 10rpx 0px 0px;
}


.buttonBox{
