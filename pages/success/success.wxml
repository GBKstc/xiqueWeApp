<view>
  <view class='img'>
    <image src='../../image/success.png' style='width:200rpx;height:200rpx'></image>
  </view>
  <view class='success' style='margin-bottom:48rpx' wx:if="{{content}}">{{successText||"预约成功"}}</view>
  <view class='content' style='margin-bottom:48rpx' wx:if="{{content}}">{{content||"预约成功"}}</view>
  <view class='success' wx:else>{{successText||"预约成功"}}</view>
  <view class='confirm' bindtap='toOrder'>{{buttonText}}</view>
</view>
